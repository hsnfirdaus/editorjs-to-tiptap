{
  "name": "editorjs-to-tiptap",
  "version": "0.0.3",
  "description": "Convert EditorJS output to TipTap JSONContent format",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prepack": "bun run build"
  },
  "keywords": [
    "editorjs",
    "tiptap",
    "converter",
    "json",
    "editor"
  ],
  "author": {
    "name": "Muhammad Hasan Firdaus",
    "email": "hasanfirdaus@protonmail.com"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@editorjs/code": "^2.9.3",
    "@editorjs/editorjs": "^2.30.8",
    "@editorjs/header": "^2.8.8",
    "@editorjs/image": "^2.10.3",
    "@editorjs/list": "^2.0.8",
    "@editorjs/paragraph": "^2.11.7",
    "@editorjs/quote": "^2.7.6",
    "@editorjs/warning": "^1.4.1",
    "@tiptap/core": "^3.0.9",
    "@types/bun": "latest",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "html-entities": "^2.6.0"
  }
}
